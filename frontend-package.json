{
  "name": "holidaibutler-frontend",
  "version": "1.0.0",
  "description": "HolidAIButler Mobile App - Costa Blanca AI Holiday Assistant",
  "main": "index.js",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "clean": "react-native clean",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios": "react-native run-ios --configuration Release",
    "bundle:android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "bundle:ios": "react-native bundle --platform ios --dev false --entry-file index.js --bundle-output ios/main.jsbundle --assets-dest ios",
    "postinstall": "npx pod-install ios"
  },
  "keywords": [
    "react-native",
    "mobile",
    "AI",
    "travel",
    "costa-blanca",
    "tourism",
    "holiday",
    "assistant"
  ],
  "author": "HolidAIButler Team",
  "license": "MIT",
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.72.6",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/drawer": "^6.6.6",
    "react-native-screens": "^3.27.0",
    "react-native-safe-area-context": "^4.7.4",
    "react-native-gesture-handler": "^2.13.4",
    "react-native-reanimated": "^3.5.4",
    "@react-native-async-storage/async-storage": "^1.19.5",
    "react-native-vector-icons": "^10.0.2",
    "react-native-svg": "^13.14.0",
    "@react-native-community/netinfo": "^9.4.1",
    "react-native-device-info": "^10.11.0",
    "react-native-permissions": "^3.10.1",
    "@reduxjs/toolkit": "^1.9.7",
    "react-redux": "^8.1.3",
    "redux-persist": "^6.0.0",
    "react-native-keychain": "^8.1.3",
    "react-native-encrypted-storage": "^4.0.3",
    "axios": "^1.6.2",
    "@tanstack/react-query": "^4.36.1",
    "socket.io-client": "^4.7.4",
    "react-native-maps": "^1.8.0",
    "@react-native-community/geolocation": "^3.2.1",
    "react-native-background-job": "^1.1.7",
    "@react-native-firebase/app": "^18.6.1",
    "@react-native-firebase/messaging": "^18.6.1",
    "@react-native-firebase/analytics": "^18.6.1",
    "@react-native-google-signin/google-signin": "^10.1.0",
    "react-native-fbsdk-next": "^12.1.2",
    "@invertase/react-native-apple-authentication": "^2.2.2",
    "react-native-voice": "^3.2.4",
    "@react-native-camera-roll/camera-roll": "^5.7.4",
    "react-native-image-picker": "^5.6.0",
    "react-native-image-resizer": "^3.0.5",
    "react-native-fast-image": "^8.6.3",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-splash-screen": "^3.3.0",
    "react-native-orientation-locker": "^1.5.0",
    "react-native-share": "^9.4.1",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-mmkv": "^2.10.2",
    "react-native-haptic-feedback": "^2.2.0",
    "react-native-system-navigation-bar": "^2.6.4",
    "react-native-status-bar-height": "^2.6.0",
    "@react-native-community/datetimepicker": "^7.6.2",
    "@react-native-picker/picker": "^2.5.1",
    "react-native-modal": "^13.0.1",
    "react-native-toast-message": "^2.1.6",
    "react-native-skeleton-placeholder": "^5.2.4",
    "lottie-react-native": "^6.4.1",
    "react-native-super-grid": "^4.9.7",
    "react-native-paper": "^5.11.1",
    "react-native-elements": "^3.4.3",
    "react-native-animatable": "^1.3.3",
    "@react-native-community/slider": "^4.4.2",
    "react-native-calendar-picker": "^7.1.3",
    "moment": "^2.29.4"
  },
  "devDependencies": {
    "@babel/core": "^7.23.3",
    "@babel/preset-env": "^7.23.3",
    "@babel/runtime": "^7.23.3",
    "@react-native/metro-config": "^0.72.11",
    "@react-native/eslint-config": "^0.72.2",
    "@types/react": "^18.2.37",
    "@types/react-native": "^0.72.6",
    "@types/react-test-renderer": "^18.0.7",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "metro-react-native-babel-preset": "^0.76.8",
    "prettier": "^3.1.0",
    "react-test-renderer": "18.2.0",
    "typescript": "^5.3.2",
    "detox": "^20.13.0",
    "@testing-library/react-native": "^12.4.2",
    "@testing-library/jest-native": "^5.4.3",
    "flipper-plugin-react-query-native-devtools": "^4.0.1",
    "reactotron-react-native": "^5.0.3",
    "reactotron-redux": "^3.1.3"
  },
  "jest": {
    "preset": "react-native",
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|@react-native|react-native-vector-icons|react-native-super-grid|react-native-reanimated|react-native-redash|@react-navigation|react-native-maps|react-native-svg|react-native-linear-gradient)/)"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts",
      "!src/types/**/*",
      "!src/**/__tests__/**/*",
      "!src/**/*.test.{js,jsx,ts,tsx}"
    ]
  },
  "prettier": {
    "arrowParens": "avoid",
    "bracketSameLine": true,
    "bracketSpacing": false,
    "singleQuote": true,
    "trailingComma": "all",
    "tabWidth": 2,
    "semi": true
  },
  "detox": {
    "testRunner": "jest",
    "runnerConfig": "e2e/config.json",
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/HolidAIButler.app",
        "type": "ios.simulator",
        "device": {
          "type": "iPhone 14"
        }
      },
      "android.emu.debug": {
        "binaryPath": "android/app/build/outputs/apk/debug/app-debug.apk",
        "type": "android.emulator",
        "device": {
          "avdName": "Pixel_4_API_30"
        }
      }
    }
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/holidaibutler.git"
  }
}